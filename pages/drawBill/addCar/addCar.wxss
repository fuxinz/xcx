/* pages/drawBill/addCar/addCar.wxss */
page{
  background-color: #ededed;
}
.address{
  width: 690rpx;
  height: 90rpx;
  line-height: 90rpx;
  margin: 50rpx auto;
  border: 1rpx solid #d7d7d7;
  background-color: #fff;
  text-align: center;
}
.license-plate{
  width: 690rpx;
  line-height: 84rpx;
  text-align: center;
}
.item-box{
  width: 84rpx;
  height: 84rpx;
  background-color: #fff;
  border: 1rpx solid #d7d7d7;
  border-right-color: transparent;
}
.btn-group{
  padding-top: 50rpx;
}
.btn{
  width: 334rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #fff;
}
.pictrue-wrap{
  margin-top: 40rpx;
  width: 400rpx;
  height: 245rpx;
  border: 1rpx solid #ccc;
  align-items: center;
  justify-content: center;
}
.pictrue-wrap .license{
  text-align: center;
  font-size: 30rpx;
}
.pictrue-wrap .pictrue{
  width: 100%;
  height: 100%;
}
.btn-confirm{
  background-color: #ff9900;
}
.btn-clear{
  background-color: #79bcff;
}