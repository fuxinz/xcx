/* pages/my/team/seeTeam.wxss */
page{
  background-color: #ededed;
  font-size: 28rpx;
}
.header{
  height: 300rpx;
  background-color: #fff;
  flex-direction: column;
  align-items: center;
}
.header-logo{
  width: 144rpx;
  height: 144rpx;
  line-height: 144rpx;
  background-color: #e4e4e4;
  text-align: center;
  border-radius: 72rpx;
  overflow: hidden;
}
.header-logo>image{
  width: 100%;
  height: 100%;
}
.header-team-name{
  font-size: 32rpx;
  padding: 20rpx;
}
.header-bottom{
  height: 48rpx;
  line-height: 48rpx;
  color: #999;
}
.header-btn{
  width: 80rpx;
  height: 48rpx;
  line-height: 48rpx;
  background-color: #ff9900;
  font-size: 24rpx;
  padding: 0;
  color: #fff;
  display: inline-block;
  align-items: center;
  margin-left: 20rpx;
}
.list-session-header{
  color: #999;
  padding: 30rpx 30rpx 20rpx;
  font-size: 32rpx;
}
.list-item{
  padding: 0 30rpx;
  height: 90rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #ebebeb;
  align-items: center;
}
.list-item .icon-main{
  width: 48rpx;
  height: 48rpx;
  padding-right: 20rpx;
}
.list-item .icon-more{
  width: 20rpx;
  height: 32rpx;
  padding-left: 20rpx;
}
.action-sheet-button{
  border-radius: 0;
  font-size: 28rpx;
  text-align: left;
}
.action-sheet-button::after{
  border: none;
}